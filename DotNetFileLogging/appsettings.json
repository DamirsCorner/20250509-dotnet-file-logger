{
  "Logging": {
    "PathFormat": "logs/log-{Date}.txt",
    "OutputTemplate": "{Timestamp:o} {RequestId,13} [{Level:u3}] {Message:lj} {Properties:j} ({EventId:x8}){NewLine}{Exception}",
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
